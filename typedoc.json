{
    "name": "SpiritLing EasyMock Common Library Documentation",
    "readme": "README.md",
    "mode": "modules",
    "out": "docs",
    "theme": "type-doc-theme",
    "ignoreCompilerErrors": "true",
    "preserveConstEnums": "true",
    "exclude": [
        "**/*+(spec).ts",
        "src/sl-easymock-lib.ts",
        "src/common.ts"
    ],
    "external-modulemap": "src/([\\w\\-_]+)/",
    "stripInternal": "false"
}